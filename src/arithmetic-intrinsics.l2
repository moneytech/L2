            ([make-literal #0 null-fragment null-expr ..] (make-asmN(r10)()..) (make-asmN(eax)()..))..) c ..])})
        
        ((" get8b) (" get) {success (do
          [generate-expression [@car (@ expr-arguments n)] gens c ..]
