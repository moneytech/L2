(function ` (l r)
	[(function aux (s t r)
		(if [emt? $s] [lllst -e- -m- -t- emt $r]

		(if (if [emt? $s] #0 (if [symbol? $s] #0 (if [emt? [@fst $s]]
			#0 (if [char= [@ffst $s] -,-] [emt? [@rfst $s]] #0))))
					[@frst $s]

		[lllllst [llllllst -i- -n- -v- -o- -k- -e- emt $r]
			[lllst -l- -s- -t- emt $r]
				(if [symbol? $s]
						[lllst --- [@fst $s] --- emt $r]
						[aux [@fst $s] $t $r])
					[aux [@rst $s] $t $r] $t emt $r]))) [@fst $l] [@frst $l] $r])
